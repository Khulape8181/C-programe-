//write a code for divison of two numbers 
 //solution:- 
 #include<stdio.h> 
 int main () 
 { 
   int x=60; 
   int y =15; 
   int div=0; 
  
   div=x/y; 
  
   printf("x value=%d\n",x); 
   printf("y value =%d\n",y); 
   printf("div value=%d\n",div); 
  
   printf("x address=%d\n",&x); 
   printf("y address=%d\n",&y); 
   printf("div address=%d\n",&div); 
   return 0; 
 }
